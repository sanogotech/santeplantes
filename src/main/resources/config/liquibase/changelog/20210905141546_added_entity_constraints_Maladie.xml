<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Maladie.
    -->
    <changeSet id="20210905141546-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="plante_id"
                                 baseTableName="maladie"
                                 constraintName="fk_maladie_plante_id"
                                 referencedColumnNames="id"
                                 referencedTableName="plante"/>

        <addForeignKeyConstraint baseColumnNames="tradipraticien_id"
                                 baseTableName="maladie"
                                 constraintName="fk_maladie_tradipraticien_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tradipraticien"/>

        <addForeignKeyConstraint baseColumnNames="traitement_id"
                                 baseTableName="maladie"
                                 constraintName="fk_maladie_traitement_id"
                                 referencedColumnNames="id"
                                 referencedTableName="traitement"/>

    </changeSet>
</databaseChangeLog>
